(de  snd_seq_open ( handle name streams mode )
     ((-gptr- "snd_seq_t **") handle )
     ((-str-) name )
     ((-int-) streams )
     ((-int-) mode)
     (int #{ snd_seq_open( $handle ,$name->data ,$streams ,$mode  )  #} )
)

(de  snd_seq_open_lconf ( handle  name  streams mode  lconf )
     ((-gptr- "snd_seq_t **")  handle )
