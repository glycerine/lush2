(setq SND_SEQ_PORT_TYPE_MIDI_GM 	      4)     ;;	/**< General MIDI compatible device */
(setq SND_SEQ_PORT_TYPE_MIDI_GS	              8)     ;;	/**< GS compatible device */
(setq SND_SEQ_PORT_TYPE_MIDI_XG	             16)     ;;	/**< XG compatible device */
(setq SND_SEQ_PORT_TYPE_MIDI_MT32            32)     ;;	/**< MT-32 compatible device */
(setq SND_SEQ_PORT_TYPE_SYNTH		   1024)     ;;	/**< Synth device */
