(setq SND_SEQ_OPEN_OUTPUT       	1)     ;;	/**< open for output (write) */
(setq SND_SEQ_OPEN_INPUT        	2)     ;;	/**< open for input (read) */
(setq SND_SEQ_OPEN_DUPLEX	        3)     ;;       /**< open for both input and output (read/write) */

(setq SND_SEQ_NONBLOCK                    1)     ;;	/**< non-blocking mode (flag to open mode) */

(setq SND_SEQ_ADDRESS_UNKNOWN		253)     ;;	/**< unknown source */
(setq SND_SEQ_ADDRESS_SUBSCRIBERS	254)     ;;	/**< send event to all subscribed ports */
(setq SND_SEQ_ADDRESS_BROADCAST	        255)     ;;	/**< send event to all queues/clients/ports/channels */

(setq SND_SEQ_CLIENT_SYSTEM		  0)     ;;	/**< system client */
(setq SND_SEQ_CLIENT_DUMMY		 62)     ;;	/**< dummy ports */
(setq SND_SEQ_CLIENT_OSS		 63)     ;;	/**< OSS sequencer emulator */


(setq SND_SEQ_USER_CLIENT                 1)        ;;	/**< user client */
(setq SND_SEQ_KERNEL_CLIENT               2)        ;;	/**< kernel client */

