(setq SND_SEQ_PORT_SYSTEM_ANNOUNCE        1)     ;;	/**< system announce port */

(setq SND_SEQ_PORT_CAP_READ		  1)     ;;     /**< readable from this port */
(setq SND_SEQ_PORT_CAP_WRITE		  2)     ;;     /**< writable to this port */
