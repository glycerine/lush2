(de gsl_block_char_alloc (   n  )
    ((-int-) n)                      ;; const size_t -> int
    (gptr #{ gsl_block_char_alloc (  $n  ) #} )   ;; gsl_block_char *  -> gptr
)

(de gsl_block_char_calloc (   n  )
    ((-int-) n)                       ;; const size_t -> int
    (gptr #{ gsl_block_char_calloc (  $n  ) #} )   ;; gsl_block_char *  -> gptr
)

(de gsl_block_char_free (   b  )
    ((-gptr- "gsl_block_char *") b)  
    #{ gsl_block_char_free (  $b  ); #} 
    ()
)


(de gsl_block_char_fread (   stream   b  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "gsl_block_char *") b)  
    (int  #{ gsl_block_char_fread (  $stream  ,$b  ) #} )
)

(de gsl_block_char_fwrite (   stream   b  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "const gsl_block_char *") b)  
    (int  #{ gsl_block_char_fwrite (  $stream  ,$b  ) #} )
)

(de gsl_block_char_fscanf (   stream   b  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "gsl_block_char *") b)  
    (int  #{ gsl_block_char_fscanf (  $stream  ,$b  ) #} )
)

(de gsl_block_char_fprintf (   stream   b   format  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "const gsl_block_char *") b) 
    ((-gptr- "const char *") format)  
    (int  #{ gsl_block_char_fprintf (  $stream  ,$b  ,$format  ) #} )
)


(de gsl_block_char_raw_fread (   stream   b   n   stride  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "char *") b) 
    ((-int-) n)                      ;; const size_t -> int
    ((-int-) stride)                       ;; const size_t -> int
    (int  #{ gsl_block_char_raw_fread (  $stream  ,$b  ,$n  ,$stride  ) #} )
)

(de gsl_block_char_raw_fwrite (   stream   b   n   stride  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "const char *") b) 
    ((-int-) n)                      ;; const size_t -> int
    ((-int-) stride)                       ;; const size_t -> int
    (int  #{ gsl_block_char_raw_fwrite (  $stream  ,$b  ,$n  ,$stride  ) #} )
)

(de gsl_block_char_raw_fscanf (   stream   b   n   stride  )
    ((-gptr- "FILE *") stream) 
    ((-gptr- "char *") b) 
    ((-int-) n)                      ;; const size_t -> int
    ((-int-) stride)                       ;; const size_t -> int
